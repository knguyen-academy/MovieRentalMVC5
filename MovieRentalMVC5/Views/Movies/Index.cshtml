@model IEnumerable<MovieRentalMVC5.Models.Movie>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Movie</h2>

<table class="table table-striped table-hover">
    <thead>
        <tr><td>Movie</td></tr>
    </thead>
    <tbody>
        @foreach (var movie in Model)
    {
        <tr class="table-primary">
            <td>@movie.Name</td>
        </tr>

}
    </tbody>
</table>

